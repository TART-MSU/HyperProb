mdp

module basic

       s   : [0..2];
       f   : [0..1];

       [] (s=0)&(f=0) -> 1/2 : (s'=1) + 1/2 : (s'=2);
       [] (s=0)&(f=0) -> 3/4 : (s'=1) + 1/4 : (s'=2);

       [] (s=0)&(f=1) -> 1/2 : (s'=1) + 1/2 : (s'=2);
       [] (s=0)&(f=1) -> 2/3 : (s'=1) + 1/3 : (s'=2);


       [] (s=1) -> 1 : (s'=1);
       [] (s=2) -> 1 : (s'=2);

endmodule

init  (s=0)  endinit

label "hg0"  = (s=0)&(f=0);
label "hle0" = (s=0)&(f=1);
label "le1"  = (s=1);
label "le2"  = (s=2);
label "end"  = (s=1)|(s=2);
label "start0" = s=0;

rewards
    (s=0)&(f=0) : 3;
    (s=0)&(f=1) : 3;
    s=1 : 1;
    s=2 : 1;
endrewards